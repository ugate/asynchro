{
  "name": "asynchro",
  "version": "4.0.0",
  "description": "Micro async lib for parallel/series/background functions using built-in ES async/await",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "lab -v -c --coverage-exclude lib -t 72 -e prod",
    "testing": "lab -v -c --coverage-exclude lib -t 72 -e test",
    "test-debug": "lab -v -c --coverage-exclude lib -t 72 -e dev",
    "jsdocp": "jsdocp ./jsdocp/jsdoc.json",
    "jsdocp-deploy": "jsdocp -D ./jsdocp/jsdoc.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ugate/asynchro.git"
  },
  "keywords": [
    "async",
    "await",
    "parallel",
    "series",
    "background",
    "concurrent",
    "sequential",
    "workflow"
  ],
  "author": "ugate",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ugate/asynchro/issues"
  },
  "homepage": "https://github.com/ugate/asynchro#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {},
  "devDependencies": {
    "@hapi/code": "^8.0.1",
    "@hapi/lab": "^22.0.4",
    "jsdocp": "^1.2.0",
    "minami": "^1.2.3"
  },
  "files": [
    "/index.js"
  ]
}
